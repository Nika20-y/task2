meta(charset="UTF-8")
link(href='rangeSlider.css' rel='stylesheet')
mixin rangeSlider(label,input1,input2)
    .rangeSlider
        .rangeSlider__header
            span.rangeSlider__label= label
            .rangeSlider__input
                .rangeSlider__input1= input1
                span.rangeSlider__separator -
                .rangeSlider__input2= input2
        .rangeSlider__item
            .rangeSlider__beetwen
            button.rangeSlider__button1
            button.rangeSlider__button2

script(src='rangeSlider.js')